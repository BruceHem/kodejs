{
  "name": "kodejs",
  "version": "1.0.0",
  "description": "Koa + Node.js = KodeJS is a web application framework.",
  "main": "index.js",
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=4.0.0"
  },
  "scripts": {
    "build": "npm run clean && mkdir -p dist && babel src -D -d dist --source-maps inline",
    "clean": "rm -rf dist",
    "dev": "nodemon src/index.js --exec babel-node",
    "doc": "apidoc -i src/app/ -o src/resource/public/doc/",
    "start": "node dist/index.js"
  },
  "author": "Bruce Hem <bruce.hem@pathmazing.com>",
  "license": "MIT",
  "dependencies": {
    "apidoc": "^0.17.5",
    "babel-runtime": "^6.22.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.7",
    "co": "^4.6.0",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "gm": "^1.23.0",
    "indicative": "^2.2.1",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "3.2.0",
    "koa-compose": "^3.1.0",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.1",
    "koa-multer": "^1.0.1",
    "koa-pug": "^3.0.0-1",
    "koa-router": "^7.1.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.14",
    "mysql": "^2.12.0",
    "sequelize": "^3.29.0",
    "shortid": "^2.2.6",
    "socket.io": "^1.7.2",
    "socket.io-redis": "^3.1.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest-minimal": "^1.1.2",
    "babel-register": "^6.18.0",
    "eslint": "^3.7.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.0",
    "nodemon": "^1.11.0"
  }
}
